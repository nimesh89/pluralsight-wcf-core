<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoLib.WindowsHost"
        mc:Ignorable="d"
        Title="MainWindow" Height="237" Width="350">
    <StackPanel Orientation="Vertical">
        <Button x:Name="BtnStart" Content="Start Service" Margin="50,20,50,0" FontSize="18" Click="BtnStart_OnClick"></Button>
        <Button x:Name="BtnStop" Content="Stop Service" Margin="50,20,50,0" FontSize="18" Click="BtnStop_OnClick"></Button>
    </StackPanel>
</Window>
